####### Build rules


all: $(TARGET)

staticlib: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	-rm -f $(TARGET)
	$(AR) $(TARGET) $(OBJECTS) $(OBJMOC)

moc: $(SRCMOC)

tmake: Makefilezaurus

Makefilezaurus: icalss.pro
	tmake icalss.pro -o Makefilezaurus

dist:
	$(TAR) icalss.tar icalss.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) icalss.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

